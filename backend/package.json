{
  "name": "cleack-backend",
  "version": "1.0.0",
  "description": "Backend API for Cleack Platform",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node --transpile-only src/index.ts",
    "build": "node build.js",
    "build:swc": "swc src -d dist --copy-files",
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "test": "jest --coverage",
    "test:unit": "jest --testPathPattern='(/__tests__/|\\.(test|spec))\\.ts$' --coverage",
    "test:integration": "jest --testPathPattern='tests/integration' --runInBand",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format:check": "prettier --check 'src/**/*.ts' 'tests/**/*.ts'",
    "format": "prettier --write 'src/**/*.ts' 'tests/**/*.ts'",
    "typecheck": "tsc --noEmit",
    "migrate": "prisma migrate deploy",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio"
  },
  "keywords": [
    "contest",
    "draw",
    "api",
    "backend"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.975.0",
    "@aws-sdk/s3-request-presigner": "^3.975.0",
    "@prisma/client": "^5.8.0",
    "@types/axios": "^0.9.36",
    "axios": "^1.13.3",
    "bcrypt": "^5.1.1",
    "bull": "^4.12.0",
    "canvas": "^3.2.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "fluent-ffmpeg": "^2.1.3",
    "gif-encoder-2": "^1.0.5",
    "glob": "^13.0.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mixpanel": "^0.20.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.8",
    "passport": "^0.7.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "pdfkit": "^0.14.0",
    "playwright": "^1.58.0",
    "redis": "^4.6.12",
    "resend": "^6.8.0",
    "sharp": "^0.34.5",
    "sib-api-v3-sdk": "^8.5.0",
    "stripe": "^19.3.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.6",
    "@faker-js/faker": "^10.2.0",
    "@swc/cli": "^0.7.10",
    "@swc/core": "^1.15.10",
    "@types/bcrypt": "^5.0.2",
    "@types/bull": "^4.10.0",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mixpanel": "^2.14.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.7",
    "@types/nodemailer": "^6.4.14",
    "@types/passport": "^1.0.16",
    "@types/passport-facebook": "^3.0.3",
    "@types/passport-google-oauth20": "^2.0.14",
    "@types/pdfkit": "^0.13.4",
    "@types/react": "^19.2.9",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "esbuild": "^0.27.2",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "prisma": "^5.8.0",
    "supertest": "^7.2.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
